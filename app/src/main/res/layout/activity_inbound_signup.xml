<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#ffffff"
    tools:context=".Inbound_signup">

    <ImageView
        android:id="@+id/logo"
        android:layout_width="281dp"
        android:layout_height="184dp"
        android:contentDescription="@string/logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.106"
        app:srcCompat="@drawable/inboundlogo" />

    <Button
        android:id="@+id/button5"
        android:layout_width="132dp"
        android:layout_height="72dp"
        android:backgroundTint="#f79123"
        android:fontFamily="@font/comfortaa"
        android:foregroundTint="#858589"
        android:text="@string/signup"
        android:textColor="#030303"
        android:textColorHighlight="#030303"
        android:textColorHint="#030303"
        android:textColorLink="#030303"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.705" />

    <Button
        android:id="@+id/button6"
        android:layout_width="132dp"
        android:layout_height="75dp"
        android:backgroundTint="#f79123"
        android:fontFamily="@font/comfortaa"
        android:foregroundTint="#858589"
        android:text="@string/back"
        android:textColor="#030303"
        android:textColorHighlight="#030303"
        android:textColorHint="#030303"
        android:textColorLink="#030303"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.836" />

    <EditText
        android:id="@+id/username"
        android:layout_width="248dp"
        android:layout_height="59dp"
        android:ems="10"
        android:fontFamily="@font/comfortaa"
        android:hint="@string/enteruser"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHighlight="#000000"
        android:textColorHint="#000000"
        android:textColorLink="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.415" />

    <EditText
        android:id="@+id/password"
        android:layout_width="239dp"
        android:layout_height="62dp"
        android:ems="10"
        android:fontFamily="@font/comfortaa"
        android:hint="@string/enterpass"
        android:inputType="textPassword"
        android:textColor="#000000"
        android:textColorHighlight="#000000"
        android:textColorHint="#000000"
        android:textColorLink="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.556" />

</androidx.constraintlayout.widget.ConstraintLayout>
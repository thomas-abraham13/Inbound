<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.inboundlogin">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/bballiconapp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bballiconapp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UserProfile"></activity>
        <activity android:name=".Bballdrills" />
        <activity android:name=".Settings" />
        <activity android:name=".HomePage" />
        <activity android:name=".Inbound_signup" />
        <activity android:name=".Inbound_login" />
        <activity android:name=".Scorekeeper" />
        <activity android:name=".PlayerInfo" />
        <activity
            android:name=".TeamRegistration"
            android:label="@string/title_activity_team_registration"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>